{
  "name": "canvas-ship",
  "version": "0.0.1",
  "description": "A canvas ship game",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-server": "cd server && nodemon -e js,hbs -w ./ -w views server.js",
    "start": "bin/prepare && (node bin/watch-assets.js & npm run watch-server & npm run watch-scripts)",
    "stop": "echo \"This just stopped\"",
    "watch-scripts": "globify \"source/js/*.js\" --outfile=.static/assets/js --verbose -w",
    "build-static-site": "bin/compile-static-to-pages.sh",
    "color": "bin/color.sh"
  },
  "author": "nils",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0",
    "browserify-shim": "^3.8.14",
    "chokidar": "^1.6.1",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "fs-extra": "^5.0.0",
    "globify": "^2.3.1",
    "hammerjs": "^2.0.8",
    "mathjs": "^3.20.2",
    "matter-attractors": "^0.1.6",
    "matter-js": "^0.14.1",
    "pixi-particles": "^3.0.0",
    "pixi.js": "^4.7.3",
    "poly-decomp": "^0.2.1",
    "turms": "^0.1.1",
    "virtualjoystick.js": "file:../virtualjoystick.js",
    "watchify": "^3.11.0"
  },
  "devDependencies": {},
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "stage-2",
            [
              "env",
              {
                "targets": {
                  "browsers": [
                    "last 2 versions"
                  ]
                }
              }
            ]
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "./node_modules/poly-decomp": "poly-decomp"
  },
  "false": {}
}
