  // type
  require('./Fraction'),

  // construction function
  require('./function/fraction')
];

},{"./Fraction":486,"./function/fraction":487}],489:[function(require,module,exports){
module.exports = [
  require('./bignumber'),
  require('./boolean'),
  require('./chain'),
  require('./complex'),
  require('./fraction'),
  require('./matrix'),
  require('./number'),
  require('./resultset'),
